{
  "version": "2.2.1-rc.3",
  "keywords": [
    "strapi",
    "plugin"
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test": "cross-env npm-run-all test:*",
    "test:unit": "jest --verbose --passWithNoTests",
    "test:integration": "jest --verbose -c jest.config.integration.ts --forceExit --detectOpenHandles",
    "test:ts": "cross-env npm-run-all test:ts:*",
    "test:ts:front": "tsc -p admin/tsconfig.json",
    "test:ts:back": "tsc -p server/tsconfig.json",
    "playground:install": "$npm_execpath run playground:yalc-add && cd playground && $npm_execpath install",
    "playground:yalc-add": "cd playground && yalc add @fourlights/strapi-plugin-deep-copy",
    "playground:yalc-add-link": "cd playground && yalc add --link @fourlights/strapi-plugin-deep-copy",
    "playground:build": "cd playground && $npm_execpath run build",
    "playground:develop": "cd playground && $npm_execpath run develop",
    "playground:start": "cd playground && $npm_execpath run start",
    "release": "release-it",
    "ci": "cross-env npm-run-all ci:*",
    "ci:admin": "biome ci admin",
    "ci:server": "biome ci server"
  },
  "dependencies": {
    "@fourlights/strapi-plugin-deep-populate": "^1.2.2",
    "@strapi/design-system": "^2.0.0-rc.14",
    "@strapi/icons": "^2.0.0-rc.14",
    "dlv": "^1.1.3",
    "dset": "^3.1.4",
    "klona": "^2.0.6",
    "pathe": "^1.1.2",
    "react-intl": "^7.1.0",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@release-it/conventional-changelog": "^9.0.4",
    "@strapi/admin": "^5.6.0",
    "@strapi/content-manager": "^5.6.0",
    "@strapi/sdk-plugin": "^5.2.8",
    "@strapi/strapi": "^5.6.0",
    "@strapi/typescript-utils": "^5.6.0",
    "@types/dlv": "^1.1.5",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "knex": "^3.1.0",
    "koa": "^2.15.3",
    "npm-run-all": "^4.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "release-it": "^17.11.0",
    "simple-git": "^3.27.0",
    "styled-components": "^6.1.13",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "@fourlights/strapi-plugin-deep-populate": "^1.2.2",
    "@strapi/admin": "^5.6.0",
    "@strapi/content-manager": "^5.6.0",
    "@strapi/design-system": "^2.0.0-rc.14",
    "@strapi/icons": "^2.0.0-rc.14",
    "@strapi/strapi": "^5.6.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "strapi": {
    "name": "deep-copy",
    "description": "This plugin enables copying nested entities including their relations.",
    "kind": "plugin",
    "displayName": "Deep Copy"
  },
  "name": "@fourlights/strapi-plugin-deep-copy",
  "description": "This plugin enables copying entities including their relations.",
  "license": "MIT",
  "author": "Four Lights <engineering@fourlights.nl> (https://github.com/Four-Lights-NL/strapi-plugin-deep-copy)",
  "maintainers": [
    "Thomas Rijpstra - Four Lights <thomas@fourlights.nl> (https://github.com/trijpstra-fourlights)"
  ],
  "homepage": "https://github.com/Four-Lights-NL/strapi-plugin-deep-copy#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Four-Lights-NL/strapi-plugin-deep-copy.git"
  },
  "bugs": {
    "url": "https://github.com/Four-Lights-NL/strapi-plugin-deep-copy/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
